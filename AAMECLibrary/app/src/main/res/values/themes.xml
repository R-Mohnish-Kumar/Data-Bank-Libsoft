<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AAMECLibrary" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/deep_sky_blue</item>
        <item name="colorPrimaryDark">@color/purple_700</item>
        <item name="colorAccent">@color/teal_200</item>
        <!-- Customize your theme here. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/deep_sky_blue</item>
    </style>
    <style name="NeumorphismCard">
        <item name="neumorph_cornerFamily">rounded</item>
        <item name="neumorph_cornerSize">@dimen/_10sdp</item>
    </style>
    <style name="NeumorphismButton">
        <item name="neumorph_cornerFamily">rounded</item>
        <item name="neumorph_cornerSize">@dimen/_30sdp</item>
    </style>
</resources>